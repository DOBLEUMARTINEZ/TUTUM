	<body>
 
		<div id="head">

			<div id="user-session" onclick="verUser();" > 
				<i class="fas fa-user"></i>
				<span id="sesion-user">{{ user }}</span>
			</div>
			
			<div class="logo"  >
				<img onclick="window.location.href='{{ asset('inicio') }}';" src='{{ asset('images/prodemex.png') }}'>
			</div>

			<div class="menu">
				<ol>
					<li onclick="window.location.href='{{ asset('inicio') }}';" >Inicio</li>

					{% if (rol == 1) or (rol == 2) %}

						<li onclick="window.location.href='{{ asset('minuta') }}';" >Minutas</li>

						<li onclick="window.location.href='{{ asset('tema') }}';" >Temas</li>

						<li onclick="window.location.href='{{ asset('usuario') }}';" >Usuarios</li>

						<li onclick="window.location.href='{{ asset('huella-digital') }}';" >Huella digital</li>

						<li onclick="window.location.href='{{ asset('configuraciones') }}';" ><i class="fas fa-cogs"></i></li>

					{% elseif rol == 3 %}

						<li onclick="window.location.href='{{ asset('minuta') }}';" >Minutas</li>

					{% endif %}

					<li onclick="window.location.href='{{ asset('login') }}';"><i class="fas fa-sign-out-alt" title="Cerrar sesion" alt="Cerrar sesion" ></i></li>

				</ol>
			</div>

		</div>

		<section id="content">
			{% block contenido %}{% endblock  %}
		</section>

