<?xml version="1.0"?>
<configuration>
	<system.webServer>
		<rewrite>
			<rules>
				
				<rule name="rule 1Q">
					<match url="^/$"  />
					<action type="Rewrite" url="/index.php"  />
				</rule>
				
				<rule name="rule 2Q" stopProcessing="true">
					<match url="^([^/]+)/?$"  />
					<action type="Rewrite" url="/index.php?seccion={R:1}&amp;%{QUERY_STRING}"  />
				</rule>

			</rules>
		</rewrite>
	</system.webServer>
</configuration>